{
	"$schema": "https://biomejs.dev/schemas/1.4.0/schema.json",
	"formatter": {
		"enabled": true,
		"formatWithErrors": false,
		"indentStyle": "tab",
		"indentWidth": 2,
		"lineWidth": 80,
		"lineEnding": "lf",
		"ignore": []
	  },
	"linter": {
		"enabled": true,
		"rules": {
		"a11y":{
			"useValidAnchor":"off"
		}
		}
	},
	"javascript":{
		"formatter":{
			"quoteStyle":"single"
			
		}
	}
}